{
  "name": "@r-wasm/webr",
  "version": "0.1.2-dev",
  "description": "The statistical programming langauge R compiled into WASM for use in a web browser and node.",
  "keywords": [
    "webR",
    "wasm",
    "R",
    "statistics",
    "programming",
    "webassembly",
    "emscripten"
  ],
  "homepage": "https://github.com/r-wasm/webr",
  "repository": {
    "type": "git",
    "url": "https://github.com/r-wasm/webr.git"
  },
  "bugs": {
    "url": "https://github.com/r-wasm/webr/issues"
  },
  "license": "SEE LICENSE IN LICENCE.md",
  "contributors": [
    {
      "name": "George Stagg",
      "email": "george.stagg@posit.co"
    },
    {
      "name": "Lionel Henry",
      "email": "lionel@posit.co"
    },
    {
      "name": "Posit Software, PBC",
      "url": "https://posit.co/"
    }
  ],
  "files": [
    "templates",
    "dist"
  ],
  "main": "dist/webr.mjs",
  "types": "webr-main.d.ts",
  "exports": {
    ".": {
      "node": "./dist/webr.cjs",
      "default": "./dist/webr.mjs"
    },
    "./chan/serviceworker": "./dist/webr-serviceworker.mjs",
    "./console": "./dist/console.mjs",
    "./repl": "./dist/repl.mjs"
  },
  "typesVersions": {
    "*": {
      "repl": [
        "dist/repl/repl"
      ],
      "repl/*": [
        "dist/repl/*"
      ],
      "console": [
        "dist/console/console"
      ],
      "console/*": [
        "dist/console/*"
      ],
      "*": [
        "dist/webR/*"
      ]
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@msgpack/msgpack": "^2.8.0",
    "@types/emscripten": "^1.39.6",
    "jquery": "^3.6.0",
    "jstree": "^3.3.12",
    "xmlhttprequest-ssl": "^2.1.0",
    "xterm": "^5.1.0",
    "xterm-addon-fit": "^0.7.0",
    "xterm-readline": "^1.1.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@types/jquery": "^3.5.14",
    "@types/jstree": "^3.3.41",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "c8": "^7.12.0",
    "esbuild": "^0.14.39",
    "esbuild-css-modules-plugin": "^2.2.16",
    "eslint": "^8.33.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jest": "^26.8.7",
    "eslint-plugin-jsdoc": "^40.0.1",
    "gts": "^3.1.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.20",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "node ./esbuild.js && tsc --emitDeclarationOnly",
    "prepack": "cp ../LICENSE.md ../README.md .",
    "postpack": "rm LICENSE.md README.md"
  }
}
